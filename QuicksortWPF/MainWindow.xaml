        xmlns:local="clr-namespace:QuicksortWPF"
        mc:Ignorable="d"
        Title="QuickSort" Height="350" Width="261.752">
    <Grid>
        <ListView x:Name="oldList" HorizontalAlignment="Left" Height="257" Margin="10,10,0,0" VerticalAlignment="Top" Width="100">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <ListView x:Name="newList" HorizontalAlignment="Left" Height="257" Margin="143,10,0,0" VerticalAlignment="Top" Width="100">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <Button Name="startBtn" Content="Start" HorizontalAlignment="Left" Margin="168,289,0,0" VerticalAlignment="Top" Width="75" Click="Start"/>
    </Grid>
</Window>
